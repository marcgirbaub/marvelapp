{
  "name": "marvelapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-native start",
